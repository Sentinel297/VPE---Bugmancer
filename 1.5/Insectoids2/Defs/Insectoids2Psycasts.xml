<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
<defName>VPE_BugworkerhiveSTNL</defName>
<label>Conjure Worker Hive</label>
<description>Use your experience with megascarabs to replicate one of their worker hives for the colony.</description>
<iconPath>Things/Building/BasicHive</iconPath>
<castTime>300</castTime>
<castSound>VPE_IceCrystal_Cast</castSound>
<range>6.9</range>
<targetMode>Location</targetMode>
<abilityClass>VFECore.Abilities.Ability_SpawnBuilding</abilityClass>
<needsTicking>true</needsTicking>
<chance>0</chance>
<modExtensions>
<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
<path>VPE_BugmancerSTNL</path>
<level>1</level>
<order>1</order>
<psyfocusCost>0.50</psyfocusCost>
<entropyGain>50</entropyGain>
<prerequisites>
				<li>VPE_MinibugSTNL</li>
				</prerequisites>
</li>
<li Class="VFECore.Abilities.AbilityExtension_Building">
<building>VFEI2_ArtificialBasicHive</building>
</li>
<li Class="VFECore.Abilities.AbilityExtension_EffecterOnTarget">
<effecterDef>Skip_EntryNoDelay</effecterDef>
<maintainForTicks>60</maintainForTicks>
<scale>1</scale>
</li>
</modExtensions>
</VFECore.Abilities.AbilityDef>

